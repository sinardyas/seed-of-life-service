{
  "extends": ["airbnb"],
  "rules": {
    "no-unused-vars": ["error", { "argsIgnorePattern": "next" }],
    "camelcase": ["off"],
    "comma-dangle": ["off"]
  },
  "env": {
    "mocha": true
  },
  "overrides": [
    {
      "files": ["test"],
      "rules": {
        "no-unused-expressions": "off"
      }
    }
  ]
}
